	record(ai,"LEBT:VMEStats:cpuUsedPercent") {
	    field(SCAN,"I/O Intr")
	    field(DTYP,"VX stats")
	    field(INP,"@cpu")
	    field(HIHI,"90.0")
	    field(HIGH,"60.0")
	    field(HHSV,"MAJOR")
	    field(HSV,"MINOR")
	    field(EGU,"%")
	}
	
	record(ai,"LEBT:VMEStats:memoryUsedPercent") {
	    field(SCAN,"I/O Intr")
	    field(DTYP,"VX stats")
	    field(INP,"@memory")
	    field(HIHI,"90.0")
	    field(HIGH,"60.0")
	    field(HHSV,"MAJOR")
	    field(HSV,"MINOR")
	    field(EGU,"%")
	}
	
	record(ai,"LEBT:VMEStats:memoryTotal") {
	    field(SCAN,"I/O Intr")
	    field(DTYP,"VX stats")
	    field(INP,"@memoryTotal")
	}
	
	record(ai,"LEBT:VMEStats:memoryUsed") {
	    field(SCAN,"I/O Intr")
	    field(DTYP,"VX stats")
	    field(INP,"@memoryUsed")
	}
	
	record(ai,"LEBT:VMEStats:memoryFree") {
	    field(SCAN,"I/O Intr")
	    field(DTYP,"VX stats")
	    field(INP,"@memoryFree")
	}
	
