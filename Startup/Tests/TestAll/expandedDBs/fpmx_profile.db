record(aSub,"DPLA:FPM01:Profile") {
    field(DESC ,"FPM profile processing")
    field(SCAN ,"Passive")
    field(SNAM,"FPM_Interleaved")
# inputs
    field(FTA, "FLOAT")
    field(NOA, "64")
    field(INPA, "DPLA:FPM01:graph NPP MS")
# outputs
    field(FTVA, "FLOAT")
    field(NOVA, "64")
    field(OUTA, "DPLA:FPM01:ProfileH PP")
}

record(waveform,"DPLA:FPM01:ProfileH")
{
	field(FTVL, "FLOAT")
	field(SCAN, "Passive")
	field(NELM, "64")
	field(DESC, "DFP01 horizontal profile")
}
