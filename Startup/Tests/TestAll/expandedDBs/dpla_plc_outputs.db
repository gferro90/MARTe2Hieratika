#################################
# Records for vacuum gauge VG01 #
# Suited for Balzers TPG300     #
#################################

# Filter time constant
# setting
record(mbbo, "DPLA:VG01:FilterSet") {
  field(DTYP, "S7plc")
  field(OUT, "@dpla_plc/6+1 T=BYTE")
  field(PINI, "YES")
  field(NOBT, "2")
  field(SHFT, "0")
  field(DESC, "Filter time constant setting")
  field(ZRVL, "0")
  field(ZRST, "No change")
  field(ZRSV, "NO_ALARM")
  field(ONVL, "1")
  field(ONST, "Fast")
  field(ONSV, "NO_ALARM")
  field(TWVL, "2")
  field(TWST, "Medium")
  field(TWSV, "NO_ALARM")
  field(THVL, "3")
  field(THST, "Slow")
  field(THSV, "NO_ALARM")
}

record(bo, "DPLA:VG01:FilterCmd") {
  field(DTYP, "S7plc")
  field(OUT, "@dpla_plc/6 T=BYTE B=2")
  field(ZNAM, "No command")
  field(ONAM, "Filter set")
  field(DESC, "Command to set filter time constant")
  field(PINI, "YES")
  field(HIGH, "1")
}

# Switching thresholds
# Upper threshold setting
# SP1, SP2, SP3, SP4 mnemonic, 0 value should be ignored
record (ao, "DPLA:VG01:UpperTHSet") {
  field(DTYP, "S7plc")
  field(PINI, "YES")
  field(OUT, "@dpla_plc/6+2 T=FLOAT32")
  field(DESC, "Upper threshold setting")
  field(PREC, "2")
  field(EGU, "mbar")
}
record(bo, "DPLA:VG01:UpperTHCmd") {
  field(DTYP, "S7plc")
  field(OUT, "@dpla_plc/6 T=BYTE B=3")
  field(ZNAM, "No command")
  field(ONAM, "Upper TH set")
  field(DESC, "Command to set upper threshold")
  field(PINI, "YES")
  field(HIGH, "1")
}

# Lower threshold setting
# SP1, SP2, SP3, SP4 mnemonic, 0 value should be ignored
record (ao, "DPLA:VG01:LowerTHSet") {
  field(DTYP, "S7plc")
  field(PINI, "YES")
  field(OUT, "@dpla_plc/6+6 T=FLOAT32")
  field(DESC, "Lower threshold setting")
  field(PREC, "2")
  field(EGU, "mbar")
}
record(bo, "DPLA:VG01:LowerTHCmd") {
  field(DTYP, "S7plc")
  field(OUT, "@dpla_plc/6 T=BYTE B=4")
  field(ZNAM, "No command")
  field(ONAM, "Lower TH set")
  field(DESC, "Command to set lower threshold")
  field(PINI, "YES")
  field(HIGH, "1")
}

# Gas correction factor
# Calibration factor relative to N2. It depends on gas and sensor type
# TPG300 does not accounts for it, it is PLC that has to divide reading by factor.
record(ao, "DPLA:VG01:GasCFSet") {
  field(DTYP, "S7plc")
  field(PINI, "YES")
  field(OUT, "@dpla_plc/6+10 T=FLOAT32")
  field(DESC, "Gas Correction Factor setting")
  field(PREC, "2")
  field(EGU, "")
  field(DRVH, "10")
  field(DRVL, "0.01")  
}
record(bo, "DPLA:VG01:GasCFCmd") {
  field(DTYP, "S7plc")
  field(OUT, "@dpla_plc/6 T=BYTE B=5")
  field(ZNAM, "No command")
  field(ONAM, "Gas CF set")
  field(DESC, "Command to set Gas Correction Factor")
  field(PINI, "YES")
  field(HIGH, "1")
}

