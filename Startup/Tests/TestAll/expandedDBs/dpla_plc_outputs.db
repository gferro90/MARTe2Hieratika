  field(DTYP, "S7plc")
  field(OUT, "@dpla_plc/0 T=UINT16 B=3")
  field(ZNAM, "No command")
  field(ONAM, "To Roughing state")
  field(DESC, "Command to go to Roughing state")
  field(PINI, "YES")
  field(HIGH, "1")
}

# Change to High Vacuum state
