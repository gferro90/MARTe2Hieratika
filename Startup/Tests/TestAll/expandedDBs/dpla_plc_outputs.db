# Change to Initial state
record(bo, "DPLA:VAC01:InitialCmd") {
  field(DTYP, "S7plc")
  field(OUT, "@dpla_plc/0 T=UINT16 B=0")
  field(ZNAM, "No command")
  field(ONAM, "To Initial state")
  field(DESC, "Command to go to Initial state")
  field(PINI, "YES")
  field(HIGH, "1")
}

# Change to Stand-by state
record(bo, "DPLA:VAC01:StandbyCmd") {
  field(DTYP, "S7plc")
  field(OUT, "@dpla_plc/0 T=UINT16 B=1")
  field(ZNAM, "No command")
  field(ONAM, "To Standby state")
  field(DESC, "Command to go to Standby state")
  field(PINI, "YES")
  field(HIGH, "1")
}

# Change to Manual state
