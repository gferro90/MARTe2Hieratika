





record(mbbiDirect, "$(mname):RM$(devno):YEAR")
{
	field(DESC, "Request Year")
	field(SCAN, "I/O Intr")
	field(DTYP, "IFMIF PLC")
	field(INP,  "@$(plc)://$(target)/DEV$(devno)M3")
	field(NOBT, "16")
	field(SHFT, "0")
}

