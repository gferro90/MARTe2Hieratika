################# Profile ###########################

record(waveform,"DPLA:FPM02:graph")
{
	field(DTYP,"asynFloat32ArrayIn")
	field(INP, "@asyn(DFP02,0)Data")
	field(SCAN,"I/O Intr")
	info(asyn:FIFO,"100")
	field(FTVL,"FLOAT")
	field(EGU,"pC")
	field(NELM,"64")
	field(PREC,"3")
	field(TSE, "-2")
}

record(mbbo,"DPLA:FPM02:Profile1")
{
	field(DESC,"First profile channels selection")
	field(DTYP,"asynInt32")
	field(OUT, "@asyn(DFP02,0)Profile1")
	info(asyn:READBACK,"1")
	field(ZRST,"None")
	field(ZRVL,"0")
# Concatenated
	field(ONST,"1..64")
	field(ONVL,"1")
	field(TWST,"64..1")
	field(TWVL,"2")
# Staggered
	field(THST,"1,33,2..63,32,64")
	field(THVL,"3")
	field(FRST,"64,32,63..2,33,1")
	field(FRVL,"4")
# First half
	field(FVST,"1..32")
	field(FVVL,"5")
	field(SXST,"32..1")
	field(SXVL,"6")
# Second half
	field(SVST,"33..64")
	field(SVVL,"7")
	field(EIST,"64..33")
	field(EIVL,"8")
}

record(mbbo,"DPLA:FPM02:Profile2")
{
	field(DESC,"Second profile channels selection")
	field(DTYP,"asynInt32")
	field(OUT, "@asyn(DFP02,0)Profile2")
	info(asyn:READBACK,"1")
	field(ZRST,"None")
	field(ZRVL,"0")
# Concatenated
	field(ONST,"1..64")
	field(ONVL,"1")
	field(TWST,"64..1")
	field(TWVL,"2")
# Staggered
	field(THST,"1,33,2..63,32,64")
	field(THVL,"3")
	field(FRST,"64,32,63..2,33,1")
	field(FRVL,"4")
# First half
	field(FVST,"1..32")
	field(FVVL,"5")
	field(SXST,"32..1")
	field(SXVL,"6")
# Second half
	field(SVST,"33..64")
	field(SVVL,"7")
	field(EIST,"64..33")
	field(EIVL,"8")
}

