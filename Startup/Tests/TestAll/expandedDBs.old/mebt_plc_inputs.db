# Flow switch
# Stat is a 2 bits word:
# LSB   --> digital input active when there is flow
# bit 1 --> digital input active when there is NOT flow
record(mbbi, "MEBT:BN01_FS:Stat") {
  field(SCAN, "I/O Intr")
  field(DTYP, "S7plc")
  field(INP, "@plc_mebt/31 T=BYTE")
  field(NOBT, "2")
  field(SHFT, "0")
  field(DESC, "Buncher BN01 flow switch state")
  field(ZRVL, "0")
  field(ZRST, "Transition")
  field(ZRSV, "MINOR")
  field(ONVL, "1")
  field(ONST, "Flow")
  field(ONSV, "NO_ALARM")
  field(TWVL, "2")
  field(TWST, "No flow")
  field(TWSV, "MINOR")
  field(THVL, "3")
  field(THST, "Short circuit")
  field(THSV, "MAJOR")
}

