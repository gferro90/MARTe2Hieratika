  field(LINR, "SLOPE")
  field(ESLO, "1.0")
  field(EOFF, "0.0")
#  field(EGU, "mm")
}

record (ai, "MEBT:BN01_PM02:Pos") {
  field(DTYP, "S7plc")
  field(SCAN, "I/O Intr")
  field(INP, "@plc_mebt/64+2 T=UINT16")
  field(DESC, "BN01 manual tuner position")
