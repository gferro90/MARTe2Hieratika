  field(LINR, "SLOPE")
  field(ESLO, "1.0")
  field(EOFF, "0.0")
#  field(EGU, "mm")
}

# Calculation of the cavity voltage CW
record(calc,"MEBT:BN01:VcavkV") {
  field(SCAN, ".5 second")
  field(INPA, "RF:LLRF5_CavB:VCavDbRdbk")
  field(CALC,"(350*10^((A-8.454)/20))")
  field(EGU,"kV")
}

# Calculation of the cavity voltage beginning T2
record(calc,"MEBT:BN01:VcavBT2kV") {
field(SCAN, ".5 second")
field(INPA, "RF:LLRF5_CavB:VCavBT2DbRdbk")
field(CALC,"(350*10^((A-8.454)/20))")
field(EGU,"kV") 
}

# Calculation of the cavity voltage end T2
