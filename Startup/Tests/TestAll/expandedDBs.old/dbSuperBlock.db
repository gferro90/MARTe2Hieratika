

record(mbbi, "$(mname):SB:ID")
{
	field(DESC, "ID")
	field(SCAN, "I/O Intr")
	field(DTYP, "IFMIF PLC")
	field(INP,  "@$(plc)://$(target)/D0001")
	field(NOBT, "16")
	field(SHFT, "0")
	
	field( ZRVL, "19009")	
	field( ONVL, "19010")	
	field( TWVL, "19011")	
	
	field( ZRST, "JAEA PLC(OLD)")
	field( ONST, "JAEA PLC")
	field( TWST, "IFMIF PLC")
}

